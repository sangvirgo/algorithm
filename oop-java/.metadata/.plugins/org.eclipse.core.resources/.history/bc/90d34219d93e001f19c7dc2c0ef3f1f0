package com.sang.stringClass;

public class Main {

	public static void main(String[] args) {
		StringBuilder sb= new StringBuilder("Sang nek");
//		at the end of string 
		sb.append(" huihihihi");
		System.out.println(sb);
//		insert at the position 
		sb.insert(8, "Java");
		System.out.println(sb);

	}

}
