package com.sang.theory2;

public class Pizza {
	private String bread;
	private String sauce;
	private String cheese;
	/**
	 * @param bread
	 * @param sauce
	 * @param cheese
	 */
	public Pizza(String bread, String sauce, String cheese) {
		super();
		this.bread = bread;
		this.sauce = sauce;
		this.cheese = cheese;
	}
	
	
}
